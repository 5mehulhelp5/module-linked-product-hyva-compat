{
    "name": "sutunam/hyva-linked-product",
    "description": "Hyva Compat For Sutunam Linked Product Module",
    "type": "magento2-module",
    "version": "1.0.4",
    "require": {
        "magento/framework": "*",
        "sutunam/module-base": "*",
        "sutunam/module-linked-product": "*"
    },
    "license": [
        "GPL-3.0-or-later"
    ],
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Sutunam\\HyvaLinkedProduct\\": "src"
        }
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "magento/composer-dependency-version-audit-plugin": false
        }
    }
}
